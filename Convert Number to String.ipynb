     "execution_count": 5,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "n = 12345\n",
    "\n",
    "def to_str(v):\n",
    "    result = ''\n",
    "    remainder = v\n",
    "    while remainder > 0:\n",
    "        val = remainder % 10\n",
    "        result = str(val) + result \n",
    "        remainder = int(remainder/10)\n",
    "    return result\n",
    "\n",
    "to_str(n)"
   ]
  }
 ],
